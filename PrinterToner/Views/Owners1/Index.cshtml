@model IEnumerable<PrinterToner.Models.Owner>

@{
    ViewBag.Title = "Index";
}

<h2>Vlasnik</h2>

<p>
    @Html.ActionLink("Unesi Novog Vlasnika", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Vlasnik
        </th>
        <th>
            Telefon
        </th>
        <th>
            Adresa
        </th>
        <th>
            Kontakt Osoba
        </th>
        <th>
            Telefon Kontakt Osobe
        </th>
        <th>
            PIB
        </th>
        <th>
            Matični Broj
        </th>
        <th>
            Firma Je u PDV-u
        </th>
        <th>
            Kreirano
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.OwnerName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OwnerTelephone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OwnerAddress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OwnerContact)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OwnerContactTelephone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OwnerPIB)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OwnerMatBroj)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OwnerIsInPDV)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Created)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.OwnerID }) |
            @Html.ActionLink("Details", "Details", new { id=item.OwnerID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.OwnerID })
        </td>
    </tr>
}

</table>
